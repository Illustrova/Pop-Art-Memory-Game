mixin character(name, bg, pattern)
	symbol(id=name)
		rect(fill="url(#" + bg + ")" height="100%" width="100%" rx="15" ry="15")
		rect(rx="15" ry="15" width="100%" height="100%" fill="url(#"+ pattern + ")")&attributes(attributes)
		image(width="100%" height="100%" xlink:href="img/" + name + ".png")

mixin radialGradient(name, stops)
	radialGradient(id=name)
		each val in stops
			stop(stop-color=val.color, offset=val.offset)/

svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="0" height="0" style="overflow: visible; display: block;")
	defs
		include clips.pug
		+radialGradient("pink-grad", [{color: "#ff649a", offset: 0}, {color: "#ff528b", offset: 0.7}, {color: "#ff3175", offset: 0.9}, {color: "#ff1b6e", offset: 1}])
		+radialGradient("blue-grad", [{color: "#57e7ff", offset: 0}, {color: "#1bd7ff", offset: 0.7}, {color: "#00bedf", offset: 1}])
		+radialGradient("yellow-grad", [{color: "#fff41f", offset: 0}, {color: "#ffec16", offset: 0.6}, {color: "#ffe007", offset: 0.8}, {color: "#ffda00", offset: 1}])
		linearGradient(id="armygreen-grad" x1="0%" y1="0%" x2="0%" y2="100%")
			stop(offset="0%" stop-color="#a9af27")/
			stop(offset="100%" stop-color="#939923")/
		linearGradient(id="aqua-grad" x1="0%" y1="0%" x2="0%" y2="100%")
			stop(offset="0%" stop-color="#3ffff1")/
			stop(offset="100%" stop-color="#15e8d3")/

		pattern(id="dots" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse")
